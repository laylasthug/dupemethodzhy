local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
    Title = "AFS - reazhy.",
    Style = 1,
    SizeX = 450,
    SizeY = 250,
    Theme = "Dark",
    ColorOverrides = {
        MainFrame = Color3.fromRGB(0,0,0)
    }
})

local Y = X.New({
    Title = "Principal"
})

local A = Y.Button({
    Text = "Você não selecionou nenhum tipo de dataloss",
    Callback = function()
        if Type == "Começar Dataloss" then
                    local args = {
                        [1] = {
                            ["1\0" .. string.rep("B", 4200000)] = require(game.ReplicatedStorage.ModuleScripts.LocalDairebStore).GetStoreProxy("GameData"):GetData("Pets")[1].UID
                        },
                        [2] = "AFS_Is_Shit",
                        [3] = 3
                    }

                    game:GetService("ReplicatedStorage").Remote.SaveTeam:FireServer(unpack(args))
            TextField:SetText("Começou")
        elseif Type == "Desfazer Dataloss" then
            for i,v in pairs(game.Players.LocalPlayer.PlayerGui.MainGui.Pets.TeamsList.Main.Scroll:GetDescendants()) do
                if v.Name == "TeamName" and v.Text == "AFS_Is_Dogshit" then
                    local args = {
                        [1] = v.Parent.LayoutOrder
                    }

                    game:GetService("ReplicatedStorage").Remote.DeleteTeam:FireServer(unpack(args))
                    TextField:SetText("Desfeito")
                    break
                end
            end
        end
    end
})

local D = Y.Dropdown({
    Text = "Tipo de Dataloss",
    Callback = function(Value)
        getgenv().Type = Value
    end,
    Options = {
        "Começar Dataloss",
        "Desfazer Dataloss"
    }
})

local AE = Y.Button({
    Text = "https://discord.gg/dTRRf2SMc4",
    Callback = function()
        print("https://discord.gg/dTRRf2SMc4")
    end
})

local AE2 = Y.Button({
    Text = "Copiar Discord Invite",
    Callback = function()
        setclipboard("https://discord.gg/dTRRf2SMc4")
    end
})

getgenv().TextField = Y.TextField({
    Text = "Status",
    Type = "Default"
})

while task.wait() do
    if Type then
        A:SetText(Type)
    end
end
